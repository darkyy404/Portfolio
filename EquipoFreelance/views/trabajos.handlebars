<link rel="stylesheet" href="/public/css/trabajos.css">

<header>
    <nav>
        <ul>
            <li><a href="/">Inicio</a></li>
            <li><a href="/miembros">Miembros</a></li>
            <li><a href="/trabajos">Trabajos</a></li>
        </ul>
    </nav>
</header>

<div class="projects-header">
    <h1>Proyectos Realizados</h1>
</div>

<div class="projects-grid">
    {{#each trabajos}}
    <div class="project-card">
        <div class="card-image">
            <img src="/public/images/{{this.imagen}}" alt="Imagen de {{this.titulo}}">
        </div>
        <div class="card-content">
            <h3>{{this.titulo}}</h3>
            <p><strong>Descripci√≥n:</strong> {{this.descripcion}}</p>
            <p><strong>Empresa:</strong> {{this.empresa}}</p>
        </div>
    </div>
    {{/each}}
</div>
